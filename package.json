{
  "name": "cslivestats-hltv",
  "version": "1.0.1",
  "description": "The unofficial HLTV Node.js API by (https://www.linkedin.com/in/lucasc0rrea/)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && npx tsc -p tsconfig.release.json",
    "start": "ts-node src/playground",
    "test": "jest --runInBand",
    "test:approve": "jest --runInBand --updateSnapshot",
    "pretty": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucascorrea/HLTV.git"
  },
  "keywords": [
    "hltv",
    "api",
    "crawler",
    "parser",
    "csgo"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Lucas Correa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucascorrea/HLTV/issues"
  },
  "homepage": "https://github.com/lucascorrea/HLTV#readme",
  "devDependencies": {
    "@types/cheerio": "0.22.35",
    "@types/jest": "29.5.13",
    "@types/node": "18.19.58",
    "@types/random-useragent": "0.3.3",
    "@types/request": "2.48.12",
    "@types/socket.io-client": "1.4.36",
    "@types/user-agents": "1.0.4",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "got-scraping": "^3.2.15",
    "socket.io-client": "2.4.0"
  }
}
